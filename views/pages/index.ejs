    <section class="slider_section">
        <div id="myCarousel" class="carousel slide banner-main" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="first-slide" src="images/main-logo2.jpg" alt="First slide">
                    <div class="container">
                        <div class="carousel-caption relative">
                            <span>All Our Meals </span>
                            <h1>prepared fresh from scratch</h1>
                            <p>Let's Celebrate
                                <br> The African Style</p>
                            <form action="/login" method="get">
                                <button type="submit" class="buynow">Login</button>
                            </form>
                            <ul class="social_icon">
                                <li> <a href="#"><i class="fa fa-facebook-f"></i></a></li>
                                <li> <a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li> <a href="#"><i class="fa fa-instagram"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="second-slide" src="images/main-logo2.jpg" alt="Second slide">
                    <div class="container">
                        <div class="carousel-caption relative">
                            <span>All Our Meals</span>
                            <h1>prepared fresh from scratch</h1>
                            <p>Let's Celebrate
                                <br> The African Style</p>
                                <form action="/login" method="get">
                                    <button type="submit" class="buynow">Login</button>
                                </form>
                            <ul class="social_icon">
                                <li> <a href="#"><i class="fa fa-facebook-f"></i></a></li>
                                <li> <a href="#"><i class="fa fa-twitter"></i></a></li>
